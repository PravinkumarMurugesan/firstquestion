void printallstudent (List<student> students)

{
for (int i=0;i< students.count;i++){
Console.WriteLine("id :"+ students[i].Id +"Name :"+student[i].Name + "Age : "+student[i].Age);

}
}

get student whose ahe is aboce 20 

List<student> getstudent (List<student> students)
{

  List<student>result = new List student>();
  for (int i=0;i< students.count;i++){
   if (student[i].age >20){
result.Add(student[i]);
}
}return result;

}


