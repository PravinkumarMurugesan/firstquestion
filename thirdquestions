
Update students ;

void update(list<Student>student,int id ,string newName ,int newAge){


int found =0;

for(int i =0 ;i<students.Count ; i++)
{

if(student [i] .Id == id){

student[i].Name = newName;
student.[i].Age = newAge;
fount=1;
Console.Writeline("student updated ");

}

if (found==0){

Console.writeline ("student not updated)

}
}
}

