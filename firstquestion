class Student {

public int id { get;set;}

public string name {get;set}

public int Age {get;set}
}

void addStudent (List student,student newstudent)

{

   int i =0;
   int duplicate =0;
while ( i < student.Count)
{
if (student.Count[id].Id ==newStudent.Id)
{

   duplicate=1;
}
i++;

if(duplicate== 0)

{
student.Add(newStudent);

Console.WriteLine("Student added");

}

else
{

Console.WriteLine("Duplicate ID,Not added.");

}
}
}
